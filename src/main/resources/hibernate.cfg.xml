<!DOCTYPE hibernate-configuration PUBLIC
    "-//Hibernate/Hibernate Configuration DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="show_sql">false</property>
    <property name="format_sql">false</property>

    <mapping class="eu.m6r.kicker.models.Channel"/>
    <mapping class="eu.m6r.kicker.models.Tournament"/>
    <mapping class="eu.m6r.kicker.models.Match"/>
    <mapping class="eu.m6r.kicker.models.Player"/>
    <mapping class="eu.m6r.kicker.models.Team"/>
    <mapping class="eu.m6r.kicker.models.PlayerSkill"/>
  </session-factory>
</hibernate-configuration>
